{
    "name": "@thesage/tokens",
    "version": "0.0.3",
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts",
        "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    }
}