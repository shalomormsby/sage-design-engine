{
    "name": "@thesage/ui",
    "version": "1.1.1",
    "description": "Sage Design Engine â€” Make it Lovable. 99 accessible React components, three themes, user-controlled motion.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "author": "Shalom Ormsby",
    "repository": {
        "type": "git",
        "url": "https://github.com/shalomormsby/ecosystem.git",
        "directory": "packages/ui"
    },
    "homepage": "https://thesage.dev",
    "bugs": "https://github.com/shalomormsby/ecosystem/issues",
    "keywords": [
        "react",
        "components",
        "ui",
        "design-system",
        "tailwind",
        "radix",
        "accessible",
        "themes",
        "mcp",
        "ai"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        ".claude",
        "README.md"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./tokens": {
            "types": "./dist/tokens.d.ts",
            "import": "./dist/tokens.mjs",
            "require": "./dist/tokens.js"
        },
        "./hooks": {
            "types": "./dist/hooks.d.ts",
            "import": "./dist/hooks.mjs",
            "require": "./dist/hooks.js"
        },
        "./utils": {
            "types": "./dist/utils.d.ts",
            "import": "./dist/utils.mjs",
            "require": "./dist/utils.js"
        },
        "./providers": {
            "types": "./dist/providers.d.ts",
            "import": "./dist/providers.mjs",
            "require": "./dist/providers.js"
        },
        "./webgl": {
            "types": "./dist/webgl.d.ts",
            "import": "./dist/webgl.mjs",
            "require": "./dist/webgl.js"
        },
        "./forms": {
            "types": "./dist/forms.d.ts",
            "import": "./dist/forms.mjs",
            "require": "./dist/forms.js"
        },
        "./dates": {
            "types": "./dist/dates.d.ts",
            "import": "./dist/dates.mjs",
            "require": "./dist/dates.js"
        },
        "./tables": {
            "types": "./dist/tables.d.ts",
            "import": "./dist/tables.mjs",
            "require": "./dist/tables.js"
        },
        "./dnd": {
            "types": "./dist/dnd.d.ts",
            "import": "./dist/dnd.mjs",
            "require": "./dist/dnd.js"
        },
        "./globals.css": "./src/globals.css"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src/",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "size": "size-limit",
        "size:check": "size-limit --limit"
    },
    "size-limit": [
        {
            "name": "Core (index)",
            "path": "dist/index.mjs",
            "limit": "450 KB"
        },
        {
            "name": "WebGL",
            "path": "dist/webgl.mjs",
            "limit": "10 KB"
        },
        {
            "name": "Forms",
            "path": "dist/forms.mjs",
            "limit": "10 KB"
        },
        {
            "name": "Dates",
            "path": "dist/dates.mjs",
            "limit": "30 KB"
        },
        {
            "name": "Tables",
            "path": "dist/tables.mjs",
            "limit": "10 KB"
        },
        {
            "name": "DnD",
            "path": "dist/dnd.mjs",
            "limit": "10 KB"
        },
        {
            "name": "Hooks",
            "path": "dist/hooks.mjs",
            "limit": "40 KB"
        },
        {
            "name": "Providers",
            "path": "dist/providers.mjs",
            "limit": "60 KB"
        },
        {
            "name": "Tokens",
            "path": "dist/tokens.mjs",
            "limit": "70 KB"
        },
        {
            "name": "Utils",
            "path": "dist/utils.mjs",
            "limit": "25 KB"
        }
    ],
    "peerDependencies": {
